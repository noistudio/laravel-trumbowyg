{
    "name": "noistudio/laravel-trumbowyg",
    "description": "Editor trumbowyg for laravel with integration Elfinder",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Artem Tatarinov",
            "email": "artem@noibiz.com"
        }

    ],
    "require": {

        "laravel/framework": "^8.12|^9.0|^10.0"
    },
    "require-dev": {
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelTrumbowyg\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTrumbowyg\\Frameworks\\Illuminate\\ServiceProvider"
            ]

        }
    },
    "minimum-stability": "dev"
}
